<!DOCTYPE html>
<html>
<head>
  <title><%= @title %></title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body id="<%= @title.gsub(/\s+/, "").downcase %>">

<%= render 'layouts/header'%>
<%= render 'layouts/nav' %>
<div class="clearfix main wrapper">
<%= yield %>

</div>
<%= render 'layouts/footer' %>
</body>
</html>
